{
  "name": "medical-image-sharing",
  "version": "1.0.0",
  "description": "Medical Image Sharing Platform",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "prisma generate && next build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "type-check": "node scripts/type-check.js",
    "property-check": "node scripts/property-consistency-check.js",
    "responsive-check": "node scripts/responsive-check.js",
    "qa": "npm run type-check && npm run property-check && npm run responsive-check",
    "qa:responsive": "npm run responsive-check",
    "qa:properties": "npm run property-check",
    "lint": "next lint",
    "use:dev": "chmod +x ./scripts/set-env.sh && ./scripts/set-env.sh development",
    "use:prod": "chmod +x ./scripts/set-env.sh && ./scripts/set-env.sh production",
    "dev:local": "npm run use:dev && next dev",
    "build:prod": "npm run use:prod && next build",
    "health": "node scripts/check-health.js",
    "health:dev": "node scripts/check-health.js --env=development",
    "health:prod": "node scripts/check-health.js --env=production",
    "test:audit-logs": "node scripts/test-audit-logging.js",
    "test:audit-logs:dev": "node scripts/test-audit-logging.js --env=development",
    "test:audit-logs:prod": "node scripts/test-audit-logging.js --env=production"
  },
  "dependencies": {
